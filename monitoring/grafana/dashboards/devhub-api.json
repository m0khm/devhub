{
  "uid": "devhub-api",
  "title": "DevHub API Overview",
  "schemaVersion": 39,
  "version": 1,
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Request Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(devhub_http_requests_total[5m]))",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(devhub_http_errors_total[5m]))",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Average Latency (s)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(devhub_http_request_duration_seconds_sum[5m])) / sum(rate(devhub_http_request_duration_seconds_count[5m]))",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 4,
      "type": "gauge",
      "title": "Active WS Connections",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "devhub_ws_connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}
